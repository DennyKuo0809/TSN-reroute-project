# seamless stream redundancy configuration
*.streamRedundancyConfigurator.configuration = [
{name: "S1-0", packetFilter: "*-type1_0-*", source: "host0", destination: "host2",trees: [[[host0, "host2"]]]},
{name: "S1-1", packetFilter: "*-type1_1-*", source: "host4", destination: "host2",trees: [[[host4, "switch3", "host2"]]]},
{name: "S2-0", packetFilter: "*-type2_0-*", source: "host0", destination: "host2",trees: [[[host0, "switch1", "switch3", "host2"]]]},
{name: "S2-1", packetFilter: "*-type2_1-*", source: "host2", destination: "host0",trees: [[[host2, "host0"]]]},
{name: "S2-2", packetFilter: "*-type2_2-*", source: "host3", destination: "host1",trees: [[[host3, "switch2", "switch0", "switch4", "host1"]]]},
{name: "S2-3", packetFilter: "*-type2_3-*", source: "host3", destination: "host2",trees: [[[host3, "host2"]]]},
{name: "S2-4", packetFilter: "*-type2_4-*", source: "host3", destination: "host4",trees: [[[host3, "switch2", "switch0", "host4"]]]},
